          "elements": [
            {
              "type": "content",
              "renderType": "html",
              "name": "Preview Label",
              "text": "<h4>Business Card Preview: </h4>",
              "visible": true,
              "renderAttributes": {
                "style": "text-align:center"
              }
            },
            {
              "type": "content",
              "renderType": "html",
              "name": "BusinessCard Preview",
              "text": "<div id=\"cardPreview\">\n</div>\n<script>\n$(function() {\n    buildCardPreview();\n    $('section[data-element-name=\"Employee Details\"] div[data-element-type=\"wrapper\"] input, section[data-element-name=\"Employee Details\"] div[data-element-type=\"wrapper\"] select').on('keyup', function(){\n      buildCardPreview();\n  });\n});\nvar logoUrl = null;\nfunction buildCardPreview(){\n    $('#cardPreview').html(\"\").append('<img id=\"cardImage\"></img>');\n    if(logoUrl === null){\n        $.get( bundle.apiLocation() + \"/kapps/\" + bundle.kappSlug() + \"?include=attributes\" , function( response ) {\n            logoAttribute = $.grep(response.kapp.attributes, function(e) { return e.name === \"Company Logo\" });\n            if(logoAttribute.length > 0){\n                logoUrl = logoAttribute[0].values[0];\n                $('#cardImage').attr('src', logoUrl);\n            } \n        });\n    } else {\n        $('#cardImage').attr('src', logoUrl);\n    }\n    $text = $('<p id=\"cardText\"></p>');\n\n    $text.append('<b>' + (K('field[First Name]').value() || \"\") + ' ' + (K('field[Middle Name]').value() || \"\")  +' ' + (K('field[Last Name]').value() || \"\") + '</b><br>');\n    $text.append('<b>' + (K('field[New Employee Title]').value() || \"\") + '</b><br>');\n    $text.append( (K('field[Address Line 1]').value() || \"\") + '<br>');\n    if ( K('field[Address Line 2]').value() != null ) {\n        $text.append(K('field[Address Line 2]').value() + '<br/>') ;\n    }\n    $text.append( (K('field[City]').value() || \"\" ) + ', ' + (K('field[State]').value() || \"\") + ' ' + (K('field[Zip]').value() || \"\" ) + '<br/>');\n    $text.append( (K('field[Office Telephone Number]').value() || \"\") + '<br/>') ;\n    if ( K('field[Employee Email Address]').value() != null ) {\n        $text.append(K('field[Employee Email Address]').value() + '<br/>') ;\n    }\n    $('#cardPreview').append($text);\n}\n</script>\n\n<style>\n \t#cardPreview {\n    width: 400px;\n    border: 1px solid;\n    padding: 10px 10px;\n    height: 200px;\n    display: inline-block;\n  }\n  #cardText {\n    float:right;\n    text-align: left;\n    margin-top: 30px;\n  }\n  #cardImage {\n    height:45px;\n    float:left;\n  }\n</style>",
              "visible": true,
              "renderAttributes": {
                "style": "text-align:center"
              }
            }
